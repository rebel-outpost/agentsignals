%html
  %head
    %meta{:name => "viewport", :content => "width=device-width, initial-scale=1.0"}
    %title= content_for?(:title) ? yield(:title) : 'Agent Signals'
    %meta{:name => "description", :content => "#{content_for?(:description) ? yield(:description) : 'Agent Signals'}"}
    %meta{ name: 'stripe-key',  content: STRIPE_PUBLIC_KEY }
    = stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true
    = javascript_include_tag 'https://js.stripe.com/v1/', "application", "data-turbolinks-track" => true
    = csrf_meta_tags

  %body
    .logo
      %h4
        %a{href: "index.html"}
          Agent
          %strong Signals
    %nav#sidebar.sidebar.nav-collapse.collapse
      %ul#side-nav.side-nav
        %li.active
          %a{href: "index.html"}
            %i.icon-home
            %span.name Dashboard
        %li
          %a{href: "index.html"}
            %i.eicon-suitcase
            %span.name Leads
        %li
          %a{href: "index.html"}
            %i.eicon-users
            %span.name Contacts
        %li
          %a{href: "index.html"}
            %i.eicon-suitcase
            %span.name Accounts
        %li
          %a{href: "index.html"}
            %i.eicon-suitcase
            %span.name Campaigns
        %li
          %a{href: "index.html"}
            %i.eicon-suitcase
            %span.name Opportunities
        %li
          %a{href: "index.html"}
            %i.icon-envelope-alt
            %span.name Email
        %li.panel
          %a.accordion-toggle.collapsed{"data-parent" => "#side-nav", "data-toggle" => "collapse", href: "#stats-collapse"}
            %i.eicon-chart-area
            %span.name Statistics
          %ul#stats-collapse.panel-collapse.collapse
            %li
              %a{href: "contact-stats.html"} Contacts
            %li
              %a{href: "listing-stats.html"} Listings
            %li
              %a{href: "deal-stats.html"} Accounts
        %li
          %a{href: "index.html"}
            %i.icon-cog
            %span.name Settings
        %li.visible-xs
          %a{href: "login.html"}
            %i.icon-signout
            %span.name Sign Out
      #sidebar-settings.settings
        %button.btn-icons.btn.btn-inverse.btn-sm{"data-value" => "icons", type: "button"} Icons
        %button.btn-auto.btn.btn-inverse.btn-sm{"data-value" => "auto", type: "button"} Auto
    .wrap
      %header.page-header
        .navbar
          %ul.nav.navbar-nav.navbar-right.pull-right
            %li.visible-phone-landscape
              %a#search-toggle{href: "#"}
